{
    "name": "semrush/php-home-test",
    "description": "Contains the sources for the Backend Engineer position tests.",
    "license": "proprietary",
    "authors": [
        {
            "name": "Semrush Inc.",
            "homepage": "https://www.semrush.com/"
        }
    ],
    "require": {
      "php": ">=8.0",
      "ext-pdo": "*",
      "doctrine/dbal": "^3.3",
      "ext-gmp": "*",
      "ext-bcmath": "*",
      "symfony/http-foundation": "^6.0",
      "symfony/http-kernel": "^6.0",
      "symfony/routing": "^6.0",
        "symfony/event-dispatcher": "^6.0",
        "symfony/dependency-injection": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "Semrush\\HomeTest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Semrush\\HomeTest\\Tests\\": "tests/"
        }
    }
}
